package evening;

import static org.junit.Assert.*;

import org.junit.*;

public class Tester {

	@Before
	public void setUp(){
		StringLoop obj = new StringLoop();
	}
	
	@Test
	public void testStringLoopPrintEven(){
		
		assertEquals(obj.printEvens(), 2);
	}
	
	@Test
	public void testStringLoopPrintOdd(){
		assertEquals(obj.printOdds(), 1);
	}
}
