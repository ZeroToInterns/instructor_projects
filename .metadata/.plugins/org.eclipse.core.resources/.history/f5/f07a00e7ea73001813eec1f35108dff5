import java.util.ArrayList;

public class ArrayListPractice {
	
	public static void main(String[] args){
		ArrayList<String> myList = new ArrayList<String>();
		myList.add("Blue");
		myList.add("Yellow");
		myList.add("Red");
		myList.add("Green");
		myList.add("Purple");
		myList.add("Black");
		myList.add("Brown");
		myList.add("Teal");
		myList.add("Rainbow");
		myList.add("Grey");
	}
	
	
	public void insertFirst(ArrayList<String> list, String color){
		list.add(0, color);
	}
}
