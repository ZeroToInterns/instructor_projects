
public class ElectronicStore {

	public static double getPrice(String item){
		if(item.equals("Macbook")){
			return 1000.99;
		}
		else if(item.equals("Phone")){
			return 899.9;
		}
		else if(item.equals("Charger")){
			return 19.99;
		}
		else if(item.equals("IPad")){
			return 699.99;
		}
		else if(item.equals("TV")){
			return 2000.99;
		}
		else{
			return 0;
		}
	}
}
