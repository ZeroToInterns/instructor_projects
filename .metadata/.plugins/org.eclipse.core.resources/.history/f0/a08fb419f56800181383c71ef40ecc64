
public class Person {
	
	//instance variables
	private static String name = "Michael";
	private static int age = 22;
	private static String hairColor = "Black";
	private static String eyeColor = "Black";
	private static double weight = 160;
	private static String height = "5'4";
	private static char gender = 'M';
	
	//main method
	public static void main(String[] args){
		
		//creates an instance of a person called newPerson
		Person newPerson = new Person();
		Person personTwo = new Person();
		System.out.println(personTwo.name);
		System.out.println(personTwo.age);
		System.out.println(personTwo.hairColor);
		System.out.println(personTwo.eyeColor);
		
		Car myCar = new Car();
